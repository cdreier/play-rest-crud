# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                       module:restcrud
#
# ~~~~

GET     /?              		        RestCrudHandler.index
GET     /{model} 				        RestCrudHandler.list
POST    /{model}				        RestCrudHandler.create
GET     /{model}/{id}			        RestCrudHandler.get
PUT     /{model}/{id}			        RestCrudHandler.update
DELETE  /{model}/{id}			        RestCrudHandler.delete